</h3> 님이 가입하신 아이디는\n\t\t\t\t\t</div>\n\t\t\t\t\t<div class=\"from_line normal\">\n\t\t\t\t\t\t<p>입니다</p>\n\t\t\t\t\t\t<p>( 가입일 : {{createDate}} )</p>\n\t\t\t\t\t</div>\n\t\t\t\t\t<div class=\"btn_box\">\n\t\t\t\t\t\t<router-link to=\"/login\" class=\"btnn grd\" >로그인</router-link>\n\t\t\t\t\t\t<router-link to=\"/login/resetPassword\" class=\"btnn\" >비밀번호 재설정</router-link>\t\n\t\t\t\t\t</div>\n\n\t\t\t\t</div>\n\t\t\t</div>\n\t\t</div>\n\t</div>\n</template>\n<script>\n\timport axios from \"axios\"\n\texport default{\n\t\tname: 'findId',\n\t\tdata() {\n\t\t\treturn {\n\t\t\t\tonlyNumber:'',\n\t\t\t\tfindIdSta: false,\n\t\t\t\tuserName: '',\n\t\t\t\tuserId: '',\n\t\t\t\tcreateDate: '',\n\t\t\t\tlogoImg: require('@/assets/images/make_pin_logo.png'),\n\t\t\t\tmdl_tkn:'',\n\t\t\t\tfrom: {\n\t\t\t\t\tuserId: '',\n\t\t\t\t\tpwd: '',\n\t\t\t\t\trepwd: '',\n\t\t\t\t\ttel: '',\n\t\t\t\t\tmobileOperator: '',\n\t\t\t\t\tbirth: '',\n\t\t\t\t\tgender: '',\n\t\t\t\t\tforeigner:'',\n\t\t\t\t\temail: '',\n\t\t\t\t\tname: '',\n\t\t\t\t\tci: '',\n\t\t\t\t},\n\t\t\t}\n\t\t},\n\t\tmethods: {\n\t\t\tphoneNum(){\n\t\t\t\treturn this.form.phone = this.form.phone.replace(/[^0-9]/g, '');\n\t\t\t},\n\n\t\t\tgetAxios(){\n\t\t\t\taxios\n\t\t\t\t.get(process.env.VUE_APP_BASE_URL + '/users/mobileUserVerifyRequest/')\n\t\t\t\t.then( res => {\n\t\t\t\t\topen_popup(res.data.mdl_tkn, this);\n\t\t\t\t})\n\t\t\t\t.catch( err => {\n\t\t\t\t\tconsole.log(err)\n\t\t\t\t});\n\t\t\t},\n\n\t\t\tsuccessPhone(){\n\t\t\t\t//테스트용으로 번호 뒤에 숫자 1넣음\n\t\t\t\tvar dataMdl_tkn = this.mdl_tkn\n\t\t\t\tdataMdl_tkn +'1'\n\t\t\t\tconsole.log(dataMdl_tkn)\n\t\t\t\taxios\n\t\t\t\t.get(process.env.VUE_APP_BASE_URL + '/users/mobileUserVerifyCheck/'+dataMdl_tkn+'/')\n\t\t\t\t.then( res => {\n\t\t\t\t\tthis.findIdData(res.data.tel)\n\t\t\t\t})\n\t\t\t\t.catch( err => {\n\t\t\t\t\tconsole.log(err)\n\t\t\t\t});\n\t\t\t},\n\n\t\t\tfindIdData(phoneNum){\n\t\t\t\tconsole.log(phoneNum)\n\t\t\t\taxios\n\t\t\t\t.get(process.env.VUE_APP_BASE_URL + '/users/findId/'+phoneNum+'/')\n\t\t\t\t.then( res => {\n\t\t\t\t\tconsole.log(res)\n\t\t\t\t\tthis.userName = res.data.name\n\t\t\t\t\tthis.userId = res.data.email\n\t\t\t\t\tconsole.log(res.data.createDate.split(\" \"))\n\t\t\t\t\tvar createDay = res.data.createDate.split(\" \")[0].split(\"-\")\n\t\t\t\t\tthis.createDate = ''+createDay[0]+'년'+createDay[1]+'월'+createDay[2]+'일'\n\t\t\t\t\tthis.findIdSta = true\n\t\t\t\t})\n\t\t\t\t.catch( err => {\n\t\t\t\t\tconsole.log(err)\n\t\t\t\t});\n\t\t\t}\n\t\t},\n\t\twatch: {\n\t\t\tmdl_tkn(val){\n\t\t\t\tthis.successPhone()\n\t\t\t\tconsole.log(val)\n\t\t\t}\n\t\t}\n\t}\n</script>\n\n<style scoped=\"\">\n.from_line_box{text-align:center;}\n.from_line{display:block;}\n.from_line h4,\n.from_line h3 {display:inline-block;}\n.from_line.user_id{color:#5077dc;}\n.from_line_box .btn_box{}\n.from_line_box .btn_box .btnn{display:inline-block;min-width:40%;}\n.from_line_box .btn_box .btnn:first-child{margin-right:2%;}\n.from_2box{border-bottom:none;}\n.btnn{min-width:100%;height:50px;line-height:50px;display:inline-block;padding:0 50px;border-radius:2em;border:1px solid rgba(0,0,0,.08);box-shadow:0 1px 6px 0 rgba(0,0,0,.1);background:#ccc;font-weight:500;color:#fff;font-size:16px;text-align:center;vertical-align:middle;cursor:pointer;overflow:visible;white-space:nowrap;margin-top:40px;margin:30px auto 0 auto;display:block}\n.btnn.grd{background-color:#498ceb;background-image:linear-gradient(45deg,#498dec 0,#6729a7 95%);box-shadow:0 1px 6px 0 rgb(94 67 183);color:#fff;border:0}\n.sub_text p{font-size:.75em;margin-bottom:.4rem;}\n</style>"]}]}