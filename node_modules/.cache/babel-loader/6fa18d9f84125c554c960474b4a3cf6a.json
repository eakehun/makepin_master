,CAAA,CAAA,CAAA;AACA;AACA;AACA,OAVA,EAWA,KAXA,CAWA,UAAA,GAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,GAAA;AACA,OAbA;AAeA,KAvEA;AAyEA,IAAA,sBAzEA,kCAyEA,GAzEA,EAyEA,IAzEA,EAyEA;AACA,UAAA,GAAA,IAAA,MAAA,EAAA;AACA,aAAA,SAAA,GAAA,IAAA,CAAA,mBAAA;AACA,aAAA,uBAAA,GAAA,IAAA,CAAA,YAAA;AACA,OAHA,MAGA,IAAA,GAAA,IAAA,MAAA,EAAA;AACA,aAAA,WAAA,GAAA,IAAA,CAAA,mBAAA;AACA,aAAA,qBAAA,GAAA,IAAA,CAAA,YAAA;AACA;;AACA,UAAA,KAAA,QAAA,IAAA,aAAA,EAAA;AACA,aAAA,YAAA,GAAA,IAAA,CAAA,YAAA;AACA,aAAA,WAAA,GAAA,IAAA,CAAA,mBAAA;AACA,OAHA,MAGA,IAAA,KAAA,QAAA,IAAA,YAAA,EAAA;AACA,aAAA,YAAA,GAAA,IAAA,CAAA,YAAA;AACA,aAAA,WAAA,GAAA,IAAA,CAAA,mBAAA;AACA;AACA;AAxFA,GAhDA;AA2IA,EAAA,KAAA,EAAA;AACA,IAAA,QAAA,EAAA,kBAAA,GAAA,EAAA;AACA,UAAA,GAAA,IAAA,aAAA,EAAA;AACA,aAAA,WAAA,GAAA,KAAA,WAAA;AACA,aAAA,YAAA,GAAA,KAAA,uBAAA;AACA,OAHA,MAGA;AACA,aAAA,WAAA,GAAA,KAAA,SAAA;AACA,aAAA,YAAA,GAAA,KAAA,qBAAA;AACA;;AACA,UAAA,KAAA,aAAA,IAAA,CAAA,EAAA,KAAA,aAAA,GAAA,KAAA,UAAA,IAAA,IAAA,KAAA,WAAA,GAAA,GAAA,CAAA;AACA,KAVA;AAWA,IAAA,UAAA,EAAA,oBAAA,GAAA,EAAA;AACA,WAAA,aAAA,GAAA,IAAA,CAAA,KAAA,CAAA,GAAA,IAAA,IAAA,KAAA,WAAA,GAAA,GAAA,CAAA,CAAA;AACA;AAbA;AA3IA,CAAA","sourcesContent":["<template>\n\t<b-container class=\"mt-md-5 mt-4 mb-md-5 mb-4\">\n\t\t<b-row>\n\t\t\t<b-col md=\"7\">\n\t\t\t\t<div class=\"ma_title\">실시간 교환현황</div>\n\t\t        <div class=\"swiper_box\">\n\t\t        \t<div class=\"sell_tbl\">\n\t\t        \t\t<div class=\"sell_th\" v-for=\"itemsName in itemsName\">{{itemsName}}</div>\n\t\t        \t</div>\n\n\t\t        \t<div class=\"swiper_container_box\">\n\t\t        \t\t<div class=\"swiper_blind\"></div>\n\t\t\t            <swiper ref=\"mySwiper\" :options=\"swiperOptions\">\n\t\t\t                <swiper-slide v-for=\"item in items\" :v-Key=\"item\">\n\t\t\t                \t<div class=\"sell_tbl\">\n\t\t\t                \t\t<div class=\"sell_td\" secondary>{{ item.createDate.substring(5, 10) }}</div>\n\t\t\t\t                \t<div class=\"sell_td\">\n\t\t\t\t                \t\t<span class=\"brand\">\n\t\t\t\t\t                \t\t<img :src=\"brandCulture\" v-if=\"item.company == 'culture'\">\n\t\t\t\t\t                \t\t<img :src=\"BrandHappy\" v-else>\n\t\t\t\t\t                \t</span>\n\t\t\t\t                \t\t {{item.userName}}\n\t\t\t\t                \t\t <span class=\"won\">{{item.requestPrice}} 원</span></div>\n\t\t\t\t                \t<div class=\"sell_td\">{{item.updateDate.substring(5, 10)}}</div>\n\t\t\t\t                \t<div class=\"sell_td time_num\"><span>{{item.withdrawCompleteDate}}</span> 초</div><!-- withdrawCompleteDate -->\n\t\t\t\t                \t<div class=\"sell_td\">\n\t\t\t\t                \t\t<span :class=\"'state_A'\" v-if=\"item.status == '처리중'\">{{item.status}}</span>\n\t\t\t\t                \t\t<span :class=\"'state_B'\" v-else-if=\"item.status == '판매완료'\">{{item.status}}</span>\n\t\t\t\t                \t\t<span :class=\"'state_B'\" v-else-if=\"item.status == '판매완료(일부)'\">{{item.status}}</span>\n\t\t\t\t                \t\t<span :class=\"'state_A'\" v-else-if=\"item.status == '판매불가'\">{{item.status}}</span>\n\t\t\t\t                \t</div>\n\t\t\t                \t</div>\n\t\t\t                </swiper-slide>\n\t\t\t            </swiper>\n\t\t        \t</div>\n\t\t        </div>\n\t\t\t</b-col>\n\t\t\t<b-col md=\"5\" class=\"mt-md-0 mt-3\">\n\t\t\t\t<div class=\"ma_title\">입금금액 계산기</div>\n\t\t\t\t<div class=\"calculator_box\">\n\t\t\t\t\t<b-form-group class=\"calu_radio pt-3\">\n\t\t\t\t\t\t<b-form-radio v-model=\"selected\" name=\"some-radios\" value=\"cultureland\">컬쳐랜드</b-form-radio>\n\t\t\t\t\t\t<b-form-radio v-model=\"selected\" name=\"some-radios\" value=\"happymoney\">해피머니</b-form-radio>\n\t\t\t\t\t</b-form-group>\n\n\t\t\t\t    <div class=\"calu_moneysum pt-3\">\n\t\t\t\t    \t<div class=\"line\">상품권 금액 합계 :</div>\n\t\t\t\t    \t<div class=\"line\"><b-form-input v-model=\"moneyPrice\" @blur=\"inputBlur\" placeholder=\"\" @click=\"zeroNum()\"></b-form-input></div>\n\t\t\t\t    </div>\n\t\t\t\t    <div class=\"calu_tbl p-3\">\n\t\t\t\t    \t<div class=\"calu_tr p-3\">\n\t\t\t\t    \t\t<div class=\"calu_td\">\n\t\t\t\t    \t\t\t<div>판매 요청 금액 :</div>\n\t\t\t\t    \t\t\t<div>{{moneyPrice}} 원</div>\n\t\t\t\t    \t\t</div>\n\t\t\t\t    \t\t<div class=\"calu_td\">\n\t\t\t\t    \t\t\t<div>매입 수수료율 :</div>\n\t\t\t\t    \t\t\t<div>{{giftboxFees}} %</div>\n\t\t\t\t    \t\t</div>\n\t\t\t\t    \t\t<div class=\"calu_td\">\n\t\t\t\t    \t\t\t<div>계좌이체 수수료 :</div>\n\t\t\t\t    \t\t\t<div>{{bankTransfer}} 원</div>\n\t\t\t\t    \t\t</div>\n\t\t\t\t    \t\t<div class=\"calu_td last_td\">\n\t\t\t\t    \t\t\t<div>입금 예정 금액 :</div>\n\t\t\t\t    \t\t\t<div><span>{{endMoneyPrice}}</span> 원</div>\n\t\t\t\t    \t\t</div>\n\t\t\t\t    \t</div>\n\t\t\t\t    </div>\n\t\t\t\t  </div>\n\t\t\t</b-col>\n\t\t</b-row>\n\t\t<b-row class=\"mt-md-4\">\n\t\t\t<b-col md=\"5\" class=\"mt-md-0 mt-3\">\n\t\t\t\t<div class=\"ma_title\">이용안내</div>\n\t\t\t</b-col>\n\t\t\t<b-col md=\"7\" class=\"mt-md-0 mt-3\">\n\t\t\t\t<div class=\"ma_title\">\n\t\t\t\t\tFAQ\n\t\t\t\t\t<span class=\"more\"></span>\n\t\t\t\t</div>\n\t\t\t\t<div class=\"faq_container\">\n\t\t\t\t\t<div class=\"list\" v-for=\"list in faqData\" :v-Key=\"list\">\n\t\t\t\t\t\t<div>{{list.title}}</div>\n\t\t\t\t\t\t<span class=\"day\">{{list.createDate}}</span>\n\t\t\t\t\t</div>\n\t\t\t\t</div>\n\t\t\t</b-col>\n\t\t</b-row>\n\t</b-container>\n</template>\n\t\t\t\t\n<script>\n\timport 'swiper/swiper-bundle.css'\n\timport axios from \"axios\"\n\n\texport default {\n\t\tname: 'containerArea',\n\t\tprops: {\n\t\t\tmsg: String\n\t\t},\n\t\tdata() {\n\t\t\treturn {\n\t\t\t\tselected: 'cultureland',\n\t\t\t\tmoneyPrice: 0,\n\t\t\t\tendMoneyPrice: 0,\n\t\t\t\tcultureFees: null,\n\t\t\t\thappyFees: null,\n\t\t\t\tgiftboxFees: 0,\n\t\t\t\tcultureFeesBankTransfer: 0,\n\t\t\t\thappyFeesBankTransfer: 0,\n\t\t\t\tbankTransfer: 0,\n\t\t\t\titemsName: ['날자', '판매자신청정보', '판매요청', '입금완료', '처리상태'],\n\t\t\t\tbrandCulture: require('@/assets/culture.jpg'),\n\t\t\t\tBrandHappy: require('@/assets/happy.jpg'),\n\t\t\t\titems: [],\n\t\t\t\tswiperOptions: {\n\t\t\t\t\tslidesPerView: 6,\n\t\t\t\t\tloop: true,\n\t\t\t\t\tautoplay: {\n\t\t\t\t\t\tdelay: 2500,\n\t\t\t\t\t\tdisableOnInteraction: true\n\t\t\t\t\t},\n\t\t\t\t\tsimulateTouch: false,\n\t\t\t\t\tdirection: 'vertical',\n\t\t\t\t},\n\t\t\t\tfaqData: '',\n\t\t\t}\n\t\t},\n\t\tcreated() {\n\t\t\tthis.purchaseFeePercents();\n\t\t\tthis.faqList();\n\t\t\tthis.tradingGift();\n\t\t},\n\t\tcomputed: {\n            swiper() {\n                return this.$refs.mySwiper.$swiper\n            }\n        },\n        mounted() {\n            //console.log('Current Swiper instance object', this.swiper)\n            //this.swiper.slideTo(3, 1000, false)\n            //this.giftboxFees = this.cultureFees\n        },\n        methods: {\n\n        \tinputBlur(){\n        \t\tif(this.moneyPrice < 10000){\n        \t\t\tthis.showMsgBoxOne('최소 1만원 이상부터 판매요청이가능합니다.')\n\t\t\t\t} else if(this.moneyPrice > 5000001){\n\t\t\t\t\tthis.moneyPrice = 5000000\n\t\t\t\t\tthis.showMsgBoxOne('1회 요청 시 최대 500백만원까지 판매요청이 가능합니다.')\n\t\t\t\t}\n        \t},\n\n        \tshowMsgBoxOne(text) {\n\t\t\t\tthis.boxOne = ''\n\t\t\t\tthis.$bvModal.msgBoxOk(text, {\n\t\t\t\t\tokTitle: '확인',\n\t\t\t\t\tokVariant: 'outline-dark',\n\t\t\t\t\tfooterClass: 'p-2 border-top-0',\n\t\t\t\t\thideHeaderClose: false,\n\t\t\t\t\tcentered: true\n\t\t\t\t})\n\t\t\t\t.then(value => {\n\t\t\t\t\tthis.boxOne = value\n\t\t\t\t})\n\t\t\t\t.catch(err => {\n\n\t\t\t\t})\n\t\t\t},\n\n        \tzeroNum: function(){\n\t\t\t\tif(this.moneyPrice == 0) this.moneyPrice = ''\n        \t},\n \n        \tfaqList(){\n        \t\taxios\n        \t\t.get(process.env.VUE_APP_BASE_URL+'/users/board/faq/?page=0&size=10', { withCredentials: true })\n        \t\t.then( res => {\n\t\t\t\t\tthis.faqData = res.data.content\n\t\t\t\t})\n        \t\t.catch( err => {\n        \t\t\tconsole.log(err);\n        \t\t});\n        \t},\n\n        \ttradingGift(){\n        \t\taxios\n        \t\t.get(process.env.VUE_APP_BASE_URL+'/users/trading/recent/?page=0&size=10&sort=idx,desc', { withCredentials: true })\n        \t\t.then( res => {\n        \t\t\tthis.items = res.data.content\n\t\t\t\t})\n        \t\t.catch( err => {\n        \t\t\tconsole.log(err);\n        \t\t});\n        \t},\n\n        \tpurchaseFeePercents(){\n        \t\tvar data = ''\n        \t\taxios\n        \t\t.get(process.env.VUE_APP_BASE_URL+'/users/fee/')\n        \t\t.then( res => {\n\t\t\t\t\tfor(var i=0;res.data.content.length>i;i++){\n\t\t\t\t\t\tif(res.data.content[i].company == \"해피머니\"){\n\t\t\t\t\t\t\tthis.purchaseFeePercentData(\"해피머니\", res.data.content[i])\n\t\t\t\t\t\t} else if(res.data.content[i].company == \"컬처랜드\"){\n\t\t\t\t\t\t\tthis.purchaseFeePercentData(\"컬처랜드\", res.data.content[i])\n\t\t\t\t\t\t}\n\t\t\t\t\t}\n\t\t\t\t})\n        \t\t.catch( err => {\n        \t\t\tconsole.log(err);\n        \t\t})\n        \t\t\n        \t},\n\n        \tpurchaseFeePercentData(obj, data){\n        \t\tif(obj == \"해피머니\"){\n\t\t\t\t\tthis.happyFees = data.purchaseFeePercents\n\t\t\t\t\tthis.cultureFeesBankTransfer = data.transperFees\n\t\t\t\t} else if(obj == \"컬처랜드\"){\n\t\t\t\t\tthis.cultureFees = data.purchaseFeePercents\n\t\t\t\t\tthis.happyFeesBankTransfer = data.transperFees\n\t\t\t\t}\n\t\t\t\tif(this.selected == 'cultureland'){\n        \t\t\tthis.bankTransfer = data.transperFees\n        \t\t\tthis.giftboxFees = data.purchaseFeePercents\n        \t\t} else if(this.selected == 'happymoney'){\n        \t\t\tthis.bankTransfer = data.transperFees\n        \t\t\tthis.giftboxFees = data.purchaseFeePercents\n        \t\t}\n        \t}\n        },\n\n        watch: {\n\t\t\tselected: function (val) {\n\t\t\t\tif(val == 'cultureland'){\n\t\t\t\t\tthis.giftboxFees = this.cultureFees\n\t\t\t\t\tthis.bankTransfer = this.cultureFeesBankTransfer\n\t\t\t\t} else {\n\t\t\t\t\tthis.giftboxFees = this.happyFees\n\t\t\t\t\tthis.bankTransfer = this.happyFeesBankTransfer\n\t\t\t\t}\n\t\t\t\tif(this.endMoneyPrice != 0) this.endMoneyPrice = this.moneyPrice * (1 - this.giftboxFees / 100)\n\t\t\t},\n\t\t\tmoneyPrice: function(val){\n\t\t\t\tthis.endMoneyPrice = Math.floor(val * (1 - this.giftboxFees / 100))\n\t\t\t}\n\t\t}\n\t}\n</script>\n\n<style>\n.container_box{}\n.ma_title{padding:15px;border:solid 1px #ddd;box-shadow:0 3px 6px 0 rgba(0,0,0,0.06);color:#444;font-weight:700;position:relative;}\n.ma_title .more{position:absolute;top:50%;right:10px;width:21px;height:21px;margin-top:-10px;border:solid 1px #999;}\n.ma_title .more:before{top:9px;right:5px;width:9px;height:1px;}\n.ma_title .more:after{top:5px;right:9px;width:1px;height:9px;}\n.ma_title .more:before,\n.ma_title .more:after{content:'';display:block;clear:both;position:absolute;background-color:#444;}\n.swiper_box{border:solid 1px #ddd;border-top:none;box-shadow:0 3px 6px 0 rgba(0,0,0,0.06);}\n.swiper_container_box{height:309px;box-shadow:0 3px 6px 0 rgba(0,0,0,0.06);position:relative;}\n.swiper_blind{position:absolute;top:0;left:0;bottom:0;width:100%;height:100%;z-index:100;}\n.swiper-container {width:100%;height:100%;}\n.swiper-slide{text-align:center;font-size:18px;background:#fff;display:-webkit-box;display:-ms-flexbox;display:-webkit-flex;display:flex;-webkit-box-pack:center;-ms-flex-pack:center;-webkit-justify-content:center;justify-content:center;-webkit-box-align:center;-ms-flex-align:center;-webkit-align-items:center;align-items:center}\n.calculator_box{border:solid 1px #ddd;border-top:none;box-shadow:0 3px 6px 0 rgba(0,0,0,0.06);}\n.calculator_box .form-group{margin-bottom:0;}\n.custom-control-label{padding:2px 0;font-size:.8em;}\n.calu_radio{}\n.calu_radio .bv-no-focus-ring{text-align:center;padding:8px 0}\n.calu_radio .bv-no-focus-ring .custom-control{display:inline-block;margin:0 10px;text-align:center;}\n.calu_moneysum{padding:0 15px;}\n.calu_moneysum:after{content:'';display:block;clear:both;}\n.calu_moneysum .line{float:left;width:50%;text-align:left;line-height:34px;font-size:.8em;}\n.c