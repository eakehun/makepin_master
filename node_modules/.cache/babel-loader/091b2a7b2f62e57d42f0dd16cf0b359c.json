K,CACJuB,GADD,CACKC,OAAO,CAACC,GAAR,CAAYC,gBAAZ,GAA6B,SADlC,EAC6C;AAACG,QAAAA,eAAe,EAAE;AAAlB,OAD7C,EAECC,IAFD,CAEO,UAAAC,GAAG,EAAI;AACbT,QAAAA,MAAM,CAAC,QAAD,CAAN;AACAW,QAAAA,OAAO,CAACC,GAAR,CAAYH,GAAG,CAACI,IAAhB;AACApC,QAAAA,MAAM,CAACsC,IAAP,CAAY;AAACD,UAAAA,IAAI,EAAE,MAAP;AAAeE,UAAAA,IAAI,EAAE;AAArB,SAAZ,EAAuC,YAAW;AACjDL,UAAAA,OAAO,CAACC,GAAR,CAAY,YAAZ;AACA,SAFD;AAGA,OARD,EASCK,KATD,CASQ,UAAAC,GAAG,EAAI;AACdP,QAAAA,OAAO,CAACC,GAAR,CAAYM,GAAG,CAACC,QAAJ,CAAaN,IAAb,CAAkBO,OAA9B;AACA,OAXD;AAYA;AArDO,GA7BoB;AAoF7BK,EAAAA,OAAO,EAAE;AApFoB,CAAf,CAAf","sourcesContent":["import Vue from 'vue'\nimport Vuex from 'vuex'\nimport router from '../router';\nimport axios from \"axios\"\nimport VueMq from \"vue-mq\"\n\nVue.use(VueMq, {\n\tbreakpoints: {\n\t\tmobile: 760,\n\t\tdesktop: Infinity,\n\t}\n})\nVue.use(Vuex)\n\nexport default new Vuex.Store({\n\tstate: {\n\t\tuserInfor: null,\n\t\tisLogin: false,\n\t\tisLoginError: false,\n\t\tdeviceType: '',\n\t},\n\tmutations: {\n\t\tlogInSuccess(state, payLoad){\n\t\t\tstate.isLogin = true\n\t\t\tstate.isLoginError = false\n\t\t\tstate.userInfor = payLoad\n\t\t},\n\n\t\tlogInError(state){\n\t\t\tstate.isLogin = false\n\t\t\tstate.isLoginError = true\n\t\t},\n\n\t\tlogOut(state){\n\t\t\tstate.isLogin = false\n\t\t\tstate.isLoginError = true\n\t\t\tstate.userInfor= null\n\t\t},\n\n\t\tdeviceValue(state, payLoad){\n\t\t\tstate.deviceType = payLoad\n\t\t}\n\t},\n\tactions: {\n\t\tlogin({ commit }, loginObj){\n\t\t\taxios\n\t\t\t.get(''+process.env.VUE_APP_BASE_URL+'/users/login/id/'+loginObj.id+'/pwd/'+loginObj.pwd+'/', { withCredentials: true })\n\t\t\t.then( res => {\n \t\t\t\tif(res.status === 200){\n \t\t\t\t\tconsole.log(res.data.name)\n \t\t\t\t\tcommit('logInSuccess', res.data.name)\n \t\t\t\t\trouter.push({name: 'Home', path: '/'}, function() {\n\t\t\t\t\t\tconsole.log(\"Home 호출 완료\");\n\t\t\t\t\t})\n \t\t\t\t}\n \t\t\t})\n\t\t\t.catch( err => {\n\t\t\t\tcommit('logInError')\n\t\t\t\tif(err.response.data.message === process.env.VUE_APP_LOGIN_MESSAGE){\n\t\t\t\t\talert(\"정보가 일치하지 않습니다.\\n다시 확인 후 시도해주세요.\")\n\t\t\t\t} else {\n\t\t\t\t\tconsole.log(2)\n\t\t\t\t}\n\t\t\t\tconsole.log(err.response.data.message);\n\t\t\t});\n\t\t},\n\n\t\tcheckSession: function({ commit }){\n\t\t\taxios\n\t\t\t.get(process.env.VUE_APP_BASE_URL+'/users/checkCurrentAuth/', { withCredentials: true })\n\t\t\t.then( res => {\n\t\t\t\tcommit('logInSuccess', res.data)\n\t\t\t})\n\t\t\t.catch( err => {\n\t\t\t\tconsole.log(err);\n\t\t\t});\n\t\t},\n\n\t\tcheckDevice: function({ commit }, data){\n\t\t\tcommit('deviceValue', data)\n\t\t},\n\n\t\tlogOut({ commit }){\n\t\t\tconsole.log(123123)\n\t\t\taxios\n\t\t\t.get(process.env.VUE_APP_BASE_URL+'/logout', {withCredentials: true})\n\t\t\t.then( res => {\n\t\t\t\tcommit('logOut')\n\t\t\t\tconsole.log(res.data);\n\t\t\t\trouter.push({name: 'Home', path: '/'}, function() {\n\t\t\t\t\tconsole.log(\"Home 호출 완료\");\n\t\t\t\t})\n\t\t\t})\n\t\t\t.catch( err => {\n\t\t\t\tconsole.log(err.response.data.message);\n\t\t\t});\n\t\t}\n\t},\n\tmodules: {\n\t}\n})\n\n"]}]}